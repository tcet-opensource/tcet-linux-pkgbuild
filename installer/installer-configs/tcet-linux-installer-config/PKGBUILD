# Maintainer: Akash6222
# Project: TCET Linux

pkgname=tcet-linux-installer-config
destname="/etc"
pkgver=23.08
pkgrel=2
pkgdesc="Calamares config for TCET Linux"
arch=('x86_64')
url="https://github.com/tcet-opensource"
license=('GPL3')
makedepends=('git')
depends=()
conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)
source=("$pkgname".tar.gz::"${url}"/"${pkgname}"/releases/download/latest/"${pkgname}".tar.gz)
sha256sums=('2e8d1380bc226cac1cce1d9c03cc97363a710ceb46d00ce0fbde572d0e731507')

package() {
	install -dm755 ${pkgdir}${destname}
	cp -r ${srcdir}/${destname}/* ${pkgdir}${destname}
}
